<script>

    function UpdateUsers() {
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetUsers", "Admin")',
            dataType: "html",
            success: function (content) {
                $("#AllUsers").html(content);

            },
            error: function (e) { },
            complete: function () {
                HideLoaderJS();
            },
            beforeSend: function () {
                LoaderJS();
            }
        });
    };
</script>